{
  "manifest_version": 2,
  "name": "Fact Checker",
  "description": "Fact checking Chrome extension",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": [
    "tabs"
  ],

  "browser_action": {
    "default_icon": "logo_small.png",
    "default_popup": "index.html"
  }
}
